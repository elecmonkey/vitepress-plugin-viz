{
  "name": "vitepress-plugin-viz",
  "version": "1.0.0",
  "description": "Add Graphviz support for VitePress using @viz-js/viz",
  "type": "module",
  "types": "./dist/index.d.mts",
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "type-check": "tsc -b --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./Viz.vue": "./dist/Viz.vue"
  },
  "keywords": [
    "vitepress",
    "plugin",
    "viz",
    "graphviz"
  ],
  "author": "Elecmonkey",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elecmonkey/vitepress-plugin-viz"
  },
  "homepage": "https://vitepress-plugin-viz.edev.uno/",
  "peerDependencies": {
    "@viz-js/viz": "^3.24.0",
    "vitepress": "^1.0.0 || ^2.0.0-0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "tsdown": "^0.20.0-beta.3",
    "vite": "^8.0.0-beta.8",
    "vitepress": "^2.0.0-alpha.15",
    "vue": "^3.5.27"
  },
  "packageManager": "pnpm@10.28.1"
}
